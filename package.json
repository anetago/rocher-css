{
  "name": "rochercss",
  "version": "0.0.2",
  "description": "Lightweight and Responsive css",
  "main": "index.js",
  "watch": {
    "autobuild": {
      "patterns": ["src"],
      "extensions": "scss"
    }
  },
  "scripts": {
    "autobuild": "rollup -c",
    "build:css": "rollup -c",
    "watch:css": "npm-watch autobuild"
